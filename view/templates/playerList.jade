h3 Players
table(class='table')
  tr
    th Name
    th Score
    th Ready
    th(ng-show='player.master') Kick
  tr(ng-repeat='(id,player) in players')
    td {{player.name}}
    td {{player.score}}
    td(class='valid-img', ng-show='player.ready')
    td(class='delete-img', ng-show='!player.ready')
    td(ng-show='player.master', class='delete-img')